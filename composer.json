{
    "name": "strucura/datagrids",
    "description": "Backend Implementation For Displaying Complex Tabular Data",
    "keywords": [
        "Strucura",
        "laravel",
        "data grids"
    ],
    "homepage": "https://github.com/strucura/datagrids",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Leach",
            "email": "7387639+andyleach@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "spatie/laravel-package-tools": "^1.16",
        "spatie/php-structure-discoverer": "^2.2"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.22.0",
        "pestphp/pest": "^3",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3||^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.1||^2.0",
        "phpstan/phpstan-phpunit": "^1.3||^2.0"
    },
    "autoload": {
        "psr-4": {
            "Strucura\\DataGrid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Strucura\\DataGrid\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Strucura\\DataGrid\\DataGridServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
