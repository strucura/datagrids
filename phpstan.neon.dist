includes:
    - phpstan-baseline.neon

parameters:
    level: 5
    paths:
        - src
        - config
    tmpDir: build/phpstan
    checkOctaneCompatibility: true
    checkModelProperties: true
    ignoreErrors:
        - '#Trait Strucura\\DataGrid\\Traits\\HasDataGridPermissions is used zero times and is not analysed.#'
